{
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
        "name": "nginx-dp"
    },
    "spec": {
        "replicas": 3,
        "revisionHistoryLimit": 10,
        "selector": {
            "matchLabels": {
                "app": "nginx"
            }
        },
        "template": {
            "metadata": {
                "labels": {
                    "app": "nginx"
                },
                "annotations": {
                    "kubernetes.io/ingress-bandwidth": "1",
                    "kubernetes.io/egress-bandwidth": "1",
                    "vnrid": "",
                    "vnrname": ""
                }
            },
            "spec": {
                "containers": [
                    {
                        "name": "nginx",
                        "image": "nginx:1.14",
                        "resources": {
                            "requests": {
                                "memory": "64",
                                "cpu": "250",
                                "ephemeral-storage": "2"
                            },
                            "limits": {
                                "memory": "128",
                                "cpu": "500",
                                "ephemeral-storage": "4"
                            }
                        },
                        "ports": [
                            {
                                "containerPort": 80
                            }
                        ]
                    }
                ]
            }
        }
    }
}